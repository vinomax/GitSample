pipeline {
    agent any

    stages {
        stage('checkout'){
            steps{
                echo 'checkout..'
            }
        }    
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
